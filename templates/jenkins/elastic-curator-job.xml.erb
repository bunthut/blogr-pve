<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description>clean up elasticsearch indices to avoid filling up the storage.</description>
  <keepDependencies>false</keepDependencies>
  <properties/>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers>
    <hudson.triggers.TimerTrigger>
      <spec>@daily</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>ssh -o StrictHostKeyChecking=no jenkins@elasticsearch.service.consul &apos;curator --config /opt/puppet/pve/files/elasticsearch/curator.yml /opt/puppet/pve/files/elasticsearch/delete_indices&apos;</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>